{
  "name": "adonis-fullstack-app",
  "version": "4.0.0",
  "adonis-version": "4.0.0",
  "description": "The fullstack application boilerplate for Adonisjs",
  "main": "index.js",
  "scripts": {
    "both": "concurrently \"adonis serve --dev\" \"yarn run development\" -n \"backend,frontend\" -c yellow,green",
    "start": "node server.js",
    "test": "NODE_ENV=testing ENV_SILENT=true node ace test",
    "development": "cross-env NODE_ENV=development webpack --watch --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "production": "cross-env NODE_ENV=production webpack --progress --hide-modules --config=node_modules/laravel-mix/setup/webpack.config.js",
    "history": "git --no-pager log --pretty=format:\"%h | %an - %ad - %s\" --date=local",
    "seed": "(adonis migration:refresh; adonis seed)"
  },
  "keywords": [
    "adonisjs",
    "adonis-app"
  ],
  "author": "",
  "license": "UNLICENSED",
  "private": true,
  "dependencies": {
    "@adonisjs/ace": "^4.0.7",
    "@adonisjs/auth": "^2.0.10",
    "@adonisjs/bodyparser": "^1.0.8",
    "@adonisjs/cors": "^1.0.2",
    "@adonisjs/fold": "^4.0.5",
    "@adonisjs/framework": "^4.0.27",
    "@adonisjs/ignitor": "^1.0.14",
    "@adonisjs/lucid": "^4.0.22",
    "@adonisjs/session": "^1.0.19",
    "@adonisjs/shield": "^1.0.4",
    "@adonisjs/validator": "^4.0.8",
    "@adonisjs/vow": "^1.0.13",
    "@adonisjs/vow-browser": "^1.0.5",
    "animate.css": "^3.5.2",
    "axios": "^0.17.1",
    "babel-polyfill": "^6.26.0",
    "balloon-css": "^0.5.0",
    "bulma": "^0.6.1",
    "chalk": "^2.3.0",
    "chart.js": "^2.7.1",
    "concurrently": "^3.5.1",
    "cross-env": "^5.1.1",
    "eslint": "^4.14.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-vue": "^4.0.0",
    "font-awesome": "^4.7.0",
    "laravel-mix": "^1.6.1",
    "lodash.orderby": "^4.6.0",
    "minireset.css": "^0.0.3",
    "moment": "^2.19.4",
    "node-schedule": "^1.2.5",
    "noty": "^3.1.4",
    "sqlite3": "^3.1.13",
    "vee-validate": "^2.0.0-rc.27",
    "vue": "^2.5.9",
    "vue-moment": "^3.1.0",
    "vue-notifications": "^0.9.0",
    "vue-router": "^3.0.1",
    "webpack": "^3.8.1"
  },
  "devDependencies": {
    "browser-sync": "^2.18.13",
    "browser-sync-webpack-plugin": "^1.2.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0"
  },
  "autoload": {
    "App": "./app"
  }
}
